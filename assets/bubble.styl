activeColor = #fff
borderColor = #777
backgroundColor = #444
inactiveColor = #ccc
shadowColor = #ddd
textColor = #fff

@import './base'
@import './bubble/*'

.ql-bubble
  box-sizing: border-box

  *
    box-sizing: border-box

  .ql-color-picker
    svg
      margin: 1px
    .ql-picker-item.ql-selected, .ql-picker-item:hover
      border-color: activeColor
